package tree;

public class Algorithm {
	
	public void pre_order(Node node) {
		if(node == null) {
			return;}
		System.out.print(node.getNodeValue() + "-> ");
		pre_order(node.getLeft());
		pre_order(node.getRight());
	}
	public void in_order(Node node) {
		if(node == null) {
			return;}
		in_order(node.getLeft());
		System.out.print(node.getNodeValue() + "-> ");
	}
	
}

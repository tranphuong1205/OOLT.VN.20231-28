package tree;

public class Node {
	private int nodeValue;
	private Node left;
	private Node right;
	
	public Node(int x) {
		this.nodeValue = x;
		this.left = null;
		this.right = null;
	}

	public int getNodeValue() {
		return nodeValue;
	}

	public void setNodeValue(int nodeValue) {
		this.nodeValue = nodeValue;
	}
	
}

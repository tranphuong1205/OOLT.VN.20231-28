package tree;

public abstract class Tree {
	private Node root;
	public Tree() {
		this.setRoot(null);
	}
	public Node getRoot() {
		return root;
	}
	public void setRoot(Node root) {
		this.root = root;
	}
	public void traverse(String algorithm) {
		
	}
	public abstract Node insert( Node  parentNode, int  newNodeValue);
	public abstract void delete(Node  node);
	public abstract void update(Node currNode, int newValue);
	public abstract Node search(int nodeValue);
	
	
	}

